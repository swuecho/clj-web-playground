{
  "name": "cljs-001",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "backend": "clj -M:backend",
    "frontend": "shadow-cljs watch web",
    "frontend:release": "shadow-cljs release web",
    "dev": "run-p backend frontend",
    "repl": "clj -M:repl",
    "sass:watch": "sass --watch public/css/users-table-rc.scss:public/css/users-table-rc.css --no-source-map",
    "sass:build": "sass --no-source-map public/css/users-table-rc.scss public/css/users-table-rc.css",
    "tailwind:build": "tailwindcss -c tailwind.config.js -i src/styles/tailwind.css -o public/css/tailwind.css --minify",
    "tailwind:watch": "tailwindcss -c tailwind.config.js -i src/styles/tailwind.css -o public/css/tailwind.css --watch",
    "build": "npm-run-all sass:build tailwind:build frontend:release"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.16",
    "daisyui": "^5.3.10",
    "npm-run-all": "^4.1.5",
    "sass": "^1.93.2",
    "shadow-cljs": "^3.2.1",
    "tailwindcss": "^4.1.16"
  },
  "dependencies": {
    "@rc-component/table": "^1.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
